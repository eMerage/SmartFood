<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayout_main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="5dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="15dp"
    android:background="@drawable/bg_history_rectangle">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_below="@+id/textview_outlet"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="10dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/divider24"
            android:layout_toRightOf="@+id/divider24">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignStart="@+id/divider26"
                android:layout_alignLeft="@+id/divider26"
                android:layout_alignParentTop="true">


                <emerge.project.onmeal.ui.fonts.RobotoMedium
                    android:id="@+id/textview_deliverd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/img_deliverd"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:lineSpacingExtra="5sp"
                    android:text="Delivered"
                    android:textColor="#de797979  "
                    android:textSize="13sp" />

                <ImageView
                    android:id="@+id/img_deliverd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/ic_deliverd_gry" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_toStartOf="@+id/divider26"
                android:layout_toLeftOf="@+id/divider26">


                <emerge.project.onmeal.ui.fonts.RobotoMedium
                    android:id="@+id/textview_transits"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/img_transits"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:lineSpacingExtra="5sp"
                    android:text="In Transit"
                    android:textColor="#de797979  "
                    android:textSize="13sp" />

                <ImageView
                    android:id="@+id/img_transits"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/ic_transit_gry" />


            </RelativeLayout>

            <View
                android:id="@+id/divider26"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:background="#e7e7e7" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/divider24"
            android:layout_toLeftOf="@+id/divider24">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/divider25"
                android:layout_toRightOf="@+id/divider25">

                <emerge.project.onmeal.ui.fonts.RobotoMedium
                    android:id="@+id/textview_make"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/img_make"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:lineSpacingExtra="5sp"
                    android:text="In the Make"
                    android:textColor="#de797979  "
                    android:textSize="13sp" />

                <ImageView
                    android:id="@+id/img_make"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/ic_inmake_gry" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_toStartOf="@+id/divider25"
                android:layout_toLeftOf="@+id/divider25">

                <emerge.project.onmeal.ui.fonts.RobotoMedium
                    android:id="@+id/textview_confirm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/img_confrimd"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:lineSpacingExtra="5sp"
                    android:text="Confirmed"
                    android:textColor="#de797979  "
                    android:textSize="13sp" />

                <ImageView
                    android:id="@+id/img_confrimd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/ic_confirmed_gry" />
            </RelativeLayout>

            <View
                android:id="@+id/divider25"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:background="#e7e7e7" />
        </RelativeLayout>

        <View
            android:id="@+id/divider24"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="#e7e7e7" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativelayout2"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_below="@+id/textview_menus"
        android:layout_alignEnd="@+id/relativelayout1"
        android:layout_alignRight="@+id/relativelayout1">


        <emerge.project.onmeal.ui.fonts.RobotoBold
            android:id="@+id/textview_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="0dp"
            android:layout_toStartOf="@+id/textview_total_cents"
            android:layout_toLeftOf="@+id/textview_total_cents"
            android:lineSpacingExtra="-3sp"
            android:text="000"
            android:textColor="#3e3e3e"
            android:textSize="18sp" />

        <emerge.project.onmeal.ui.fonts.RobotoBold
            android:id="@+id/textview_total_cents"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="2dp"
            android:lineSpacingExtra="1sp"
            android:text=".00"
            android:textColor="#3e3e3e"
            android:textSize="14sp" />


    </RelativeLayout>

    <emerge.project.onmeal.ui.fonts.RobotoRegular
        android:id="@+id/textview_dispatch_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_menus"
        android:layout_alignStart="@+id/relativelayout1"
        android:layout_alignLeft="@+id/relativelayout1"
        android:layout_marginTop="5dp"
        android:lineSpacingExtra="5sp"
        android:textColor="#dd432b"
        android:textSize="14sp" />


    <emerge.project.onmeal.ui.fonts.RobotoRegular
        android:id="@+id/textview_outlet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_dispatch_type"
        android:layout_alignStart="@+id/relativelayout1"
        android:layout_alignLeft="@+id/relativelayout1"
        android:layout_marginTop="5dp"
        android:lineSpacingExtra="5sp"
        android:textColor="#3e3e3e"
        android:text=""
        android:textSize="14sp" />



    <emerge.project.onmeal.ui.fonts.RobotoRegular
        android:id="@+id/textview_menus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativelayout1"
        android:layout_alignStart="@+id/relativelayout1"
        android:layout_alignLeft="@+id/relativelayout1"
        android:layout_marginTop="5dp"
        android:lineSpacingExtra="5sp"
        android:textColor="#de797979"
        android:textSize="12sp" />

    <RelativeLayout
        android:id="@+id/relativelayout1"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="15dp">

        <emerge.project.onmeal.ui.fonts.RobotoMedium
            android:id="@+id/robotoMedium24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:lineSpacingExtra="2sp"
            android:text="Current Order"
            android:textColor="#7ed321"
            android:textSize="12sp" />

        <emerge.project.onmeal.ui.fonts.RobotoMedium
            android:id="@+id/textview_orderoutletdetails"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toStartOf="@+id/robotoMedium24"
            android:layout_toLeftOf="@+id/robotoMedium24"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:textColor="#de262626"
            android:text="Outlet Details"
            android:paddingLeft="40dp"
            android:textSize="14sp" />

        <emerge.project.onmeal.ui.fonts.RobotoMedium
            android:id="@+id/textview_ordernumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:lineSpacingExtra="1sp"
            android:textColor="#de262626"
            android:textSize="14sp" />
    </RelativeLayout>
</RelativeLayout>