<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="120dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="10dp"
    android:background="@drawable/bg_favorites">


    <RelativeLayout
        android:id="@+id/relativeLayout5_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_favorites">


        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/ic_remove_fav" />

        <emerge.project.onmeal.ui.fonts.RobotoRegular
            android:id="@+id/robotoRegular8"
            android:layout_width="wrap_content"
            android:layout_height="16dp"
            android:layout_below="@+id/robotoRegular7"
            android:layout_alignStart="@+id/robotoRegular7"
            android:layout_alignLeft="@+id/robotoRegular7"
            android:layout_marginTop="5dp"
            android:lineSpacingExtra="5sp"
            android:textColor="#de797979"
            android:textSize="13sp" />


        <emerge.project.onmeal.ui.fonts.RobotoMedium
            android:id="@+id/robotoRegular7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="30dp"
            android:layout_toStartOf="@+id/imageView16"
            android:layout_toLeftOf="@+id/imageView16"
            android:layout_toEndOf="@+id/imageview_list_menu_image"
            android:layout_toRightOf="@+id/imageview_list_menu_image"
            android:lineSpacingExtra="1sp"
            android:textColor="#de262626"
            android:textSize="16sp" />

        <com.mikhaellopez.circularimageview.CircularImageView
            android:id="@+id/imageview_list_menu_image"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerVertical="true"
            android:src="@drawable/ic_image_default"
            app:civ_border_color="#00000000" />


    </RelativeLayout>


</RelativeLayout>
