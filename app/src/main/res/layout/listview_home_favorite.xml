<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="190dp"
    android:layout_height="85dp"
    android:layout_marginRight="15dp">

    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:clickable="true"
        android:elevation="2dp"
        android:foreground="?android:attr/selectableItemBackground"
        card_view:cardCornerRadius="12dp"
        card_view:cardPreventCornerOverlap="false"
        android:layout_alignParentLeft="true">

        <RelativeLayout
            android:id="@+id/relativeLayout5_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_below="@+id/robotoRegular7"
                android:layout_alignStart="@+id/robotoRegular7"
                android:layout_alignLeft="@+id/robotoRegular7"
                android:layout_marginTop="5dp">

                <emerge.project.onmeal.ui.fonts.RobotoRegular
                    android:id="@+id/robotoRegular8"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:textSize="11sp" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:layout_toEndOf="@+id/robotoRegular8"
                    android:layout_toRightOf="@+id/robotoRegular8"
                    android:src="@drawable/ic_heart_fav" />


            </RelativeLayout>


            <emerge.project.onmeal.ui.fonts.RobotoMedium
                android:id="@+id/robotoRegular7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="3dp"
                android:layout_toEndOf="@+id/imageview_list_menu_image"
                android:layout_toRightOf="@+id/imageview_list_menu_image"
                android:lineSpacingExtra="1sp"
                android:textColor="#de262626"
                android:textSize="13sp" />

            <com.mikhaellopez.circularimageview.CircularImageView
                android:id="@+id/imageview_list_menu_image"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_image_default"
                app:civ_border_color="#00000000" />


        </RelativeLayout>
    </android.support.v7.widget.CardView>

</RelativeLayout>
